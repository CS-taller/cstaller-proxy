{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "https://cstaller.myshopify.com/$1"
    }
  ],
  "redirects": [
    {
      "source": "/checkout",
      "destination": "https://cstaller.myshopify.com/checkout",
      "permanent": false
    },
    {
      "source": "/checkout/(.*)",
      "destination": "https://cstaller.myshopify.com/checkout/$1",
      "permanent": false
    }
  ]
}
